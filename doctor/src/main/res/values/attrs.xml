<?xml version="1.0" encoding="utf-8"?>
<resources>
  <declare-styleable name="CircleProgressBar">
    <attr name="progress" format="integer" />
    <attr name="progress_color" format="color|reference" />
    <attr name="progress_bgcolor" format="color|reference" />
    <attr name="progress_width" format="dimension"/>
    <attr name="progress_bg_width" format="dimension"/>
  </declare-styleable>

  <declare-styleable name="TextViewVertical">
    <attr name="v_text" format="string" />
    <attr name="v_textSize" format="dimension" />
    <attr name="v_textColor" format="color" />
    <attr name="v_start" format="enum">
      <enum name="left" value="1" />
      <enum name="right" value="2" />
    </attr>
    <attr name="v_line" format="enum">
      <enum name="none" value="0" />
      <enum name="left" value="1" />
      <enum name="right" value="2" />
    </attr>
    <attr name="v_lineWidth" format="dimension" />
    <attr name="v_lineColor" format="color" />
    <attr name="v_textVerticalMargin" format="dimension" />
    <attr name="v_textHorizontalMargin" format="dimension" />
    <attr name="v_cutChars" format="string" />
    <attr name="v_line2TextMargin" format="dimension" />
  </declare-styleable>
</resources>